{
    "name": "Dracula",
    "author": "Zeno Rocha",
    "semanticClass": "theme.dracula",
    "scheme": {
        "BG": "#282a36",
        "FG": "#f8f8f2",
        "SELECTION": "#44475a",
        "COMMENT": "#6272a4",
        "CYAN": "#8be9fd",
        "GREEN": "#50fa7b",
        "ORANGE": "#ffb86c",
        "PINK": "#ff79c6",
        "PURPLE": "#bd93f9",
        "RED": "#ff5555",
        "YELLOW": "#f1fa8c"
    },
    "settings": [
        {
            "settings": {
                "background": "#282a36",
                "foreground": "#f8f8f2",
                "lineHighlight": "#44475a",
                "findRangeHighlight": null,
                "inactiveSelection": null,
                "invisibles": null,
                "selection": "#44475a",
                "guide": null,
                "rangeHighlight": null,
                "selectionHighlight": null,
                "wordHighlight": "#6272a4",
                "wordHighlightStrong": "#6272a4",
                "findMatchHighlight": "#44475a",
                "currentFindMatchHighlight": "#6272a4",
                "linkForeground": null,
                "activeLinkForeground": "#50fa7b",
                "hoverHighlight": "#44475a",
                "referenceHighlight": null
            }
        },
        {
            "scope": "emphasis",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "strong",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "scope": "header",
            "settings": {
                "foreground": "#ff79c6"
            }
        },
        {
            "scope": "comment",
            "settings": {
                "foreground": "#6272a4"
            }
        },
        {
            "scope": "source",
            "settings": {
                "foreground": "#f8f8f2"
            }
        },
        {
            "scope": "contant.language",
            "settings": {
                "foreground": "#50fa7b"
            }
        },
        {
            "scope": [
                "contant.numeric",
                "constant.numeric.decimal",
                "constant.numeric.scss",
                "constant.numeric.css",
                "constant.numeric.less",
                "constant.numeric.color.rgb-value"
            ],
            "settings": {
                "foreground": "#bd93f9"
            }
        },
        {
            "scope": "constant.regexp",
            "settings": {
                "foreground": "#ff79c6"
            }
        },
        {
            "scope": "variable",
            "settings": {
                "foreground": "#f8f8f2"
            }
        },
        {
            "scope": "variable.language.this",
            "settings": {
                "foreground": "#bd93f9",
                "fontStyle": "bold italic"
            }
        },
        {
            "scope": "variable.parameter",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#ffb86c"
            }
        },
        {
            "scope": [
                "keyword",
                "punctuation.definition.keyword"
            ],
            "settings": {
                "foreground": "#ff79c6"
            }
        },
        {
            "scope": "keyword.operator.new",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "meta.diff",
                "meta.diff.header"
            ],
            "settings": {
                "foreground": "#6272a4"
            }
        },
        {
            "scope": "markup.underline",
            "settings": {
                "fontStyle": "underline"
            }
        },
        {
            "scope": "markup.bold",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#ffb86c"
            }
        },
        {
            "scope": "markup.heading",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#bd93f9"
            }
        },
        {
            "scope": "markup.italic",
            "settings": {
                "foreground": "#f1fa8c",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "markup.inserted",
            "settings": {
                "foreground": "#50fa7b"
            }
        },
        {
            "scope": "markup.deleted",
            "settings": {
                "foreground": "#ff5555"
            }
        },
        {
            "scope": "markup.changed",
            "settings": {
                "foreground": "#ffb86c"
            }
        },
        {
            "scope": "invalid",
            "settings": {
                "background": "#ff5555",
                "foreground": "#f8f8f2"
            }
        },
        {
            "scope": "invalid.deprecated",
            "settings": {
                "background": "#bd93f9",
                "foreground": "#f8f8f2"
            }
        },
        {
            "scope": "entity.name.filename",
            "settings": {
                "foreground": "#f1fa8c"
            }
        },
        {
            "scope": "markup.error",
            "settings": {
                "foreground": "#ff5555"
            }
        },
        {
            "scope": [
                "beginning.punctuation.definition.quote.markdown",
                "beginning.punctuation.definition.list.markdown"
            ],
            "settings": {
                "foreground": "#8be9fd"
            }
        },
        {
            "scope": "markup.inline.raw",
            "settings": {
                "foreground": "#50fa7b"
            }
        },
        {
            "scope": [
                "markup.underline.link.markdown",
                "markup.underline.link.image.markdown"
            ],
            "settings": {
                "foreground": "#8be9fd"
            }
        },
        {
            "scope": [
                "string.other.link.description.markdown",
                "string.other.link.title.markdown"
            ],
            "settings": {
                "foreground": "#ff79c6"
            }
        },
        {
            "scope": "markup.quote.markdown",
            "settings": {
                "foreground": "#6272a4"
            }
        },
        {
            "scope": "meta.separator.markdown",
            "settings": {
                "foreground": "#6272a4"
            }
        },
        {
            "scope": "punctuation.definition.markdown",
            "settings": {
                "foreground": "#50fa7b"
            }
        },
        {
            "scope": "fenced_code.block.language",
            "settings": {
                "foreground": "#ffb86c"
            }
        },
        {
            "scope": "meta.selector",
            "settings": {
                "foreground": "#f8f8f2"
            }
        },
        {
            "scope": "meta.preprocessor",
            "settings": {
                "foreground": "#8be9fd"
            }
        },
        {
            "scope": "meta.preprocessor.string",
            "settings": {
                "foreground": "#ffb86c"
            }
        },
        {
            "scope": "meta.preprocessor.numeric",
            "settings": {
                "foreground": "#bd93f9"
            }
        },
        {
            "scope": "storage",
            "settings": {
                "foreground": "#ff79c6"
            }
        },
        {
            "scope": "storage.type",
            "settings": {
                "foreground": "#8be9fd",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "storage.modifier",
            "settings": {
                "foreground": "#ff79c6"
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "#f1fa8c"
            }
        },
        {
            "scope": "string.tag",
            "settings": {
                "foreground": "#f1fa8c"
            }
        },
        {
            "scope": "string.value",
            "settings": {
                "foreground": "#f1fa8c"
            }
        },
        {
            "scope": "string.regexp",
            "settings": {
                "foreground": "#ff5555"
            }
        },
        {
            "name": "Provided by external / built-in library",
            "scope": [
                "support.function"
            ],
            "settings": {
                "foreground": "#8be9fd"
            }
        },
        {
            "name": "Defined language constants",
            "scope": "support.constant",
            "settings": {
                "foreground": "#8be9fd"
            }
        },
        {
            "name": "Defined language constant properties",
            "scope": "support.constant.property",
            "settings": {
                "foreground": "#bd93f9"
            }
        },
        {
            "name": "External / built-in types and classes",
            "scope": [
                "support.type",
                "support.class"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#8be9fd"
            }
        },
        {
            "name": "Functions",
            "scope": "entity.name.function",
            "settings": {
                "foreground": "#50fa7b"
            }
        },
        {
            "name": "Inherited classes",
            "scope": "entity.other.inherited-class",
            "settings": {
                "fontStyle": "italic underline",
                "foreground": "#50fa7b"
            }
        },
        {
            "name": "Decorators",
            "scope": [
                "meta.decorator",
                "meta.decorator variable.other.readwrite"
            ],
            "settings": {
                "foreground": "#8be9fd",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "support.constant.property-value",
                "support.constant.color",
                "support.constant.font-name",
                "support.constant.media-type",
                "support.constant.media"
            ],
            "settings": {
                "foreground": "#f8f8f2"
            }
        },
        {
            "scope": [
                "constant.numeric.color.hex-value",
                "constant.other.color.rgb-value",
                "constant.other.rgb-value"
            ],
            "settings": {
                "foreground": "#f1fa8c"
            }
        },
        {
            "scope": [
                "variable.css",
                "variable.scss",
                "variable.stylus",
                "variable.other.less"
            ],
            "settings": {
                "foreground": "#ffb86c"
            }
        },
        {
            "scope": [
                "meta.property-list.css",
                "support.type.vendored.property-name",
                "support.type.property-name"
            ],
            "settings": {
                "foreground": "#8be9fd"
            }
        },
        {
            "name": "HTML tags in CSS",
            "scope": "entity.name.tag.css",
            "settings": {
                "foreground": "#ff79c6"
            }
        },
        {
            "scope": [
                "entity.other.attribute-name.parent-selector.css",
                "entity.other.attribute-name.pseudo-class.css",
                "entity.other.attribute-name.pseudo-element.css"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#50fa7b"
            }
        },
        {
            "scope": [
                "entity.other.attribute-name.id.css",
                "source.css.less entity.other.attribute-name.id",
                "entity.other.attribute-name.attribute.scss",
                "entity.other.attribute-name.scss",
                "entity.other.attribute-name.class.css",
                "entity.other.attribute-name.class.mixin.css"
            ],
            "settings": {
                "foreground": "#50fa7b"
            }
        },
        {
            "name": "HTML attributes",
            "scope": "entity.other.attribute-name",
            "settings": {
                "foreground": "#50fa7b"
            }
        },
        {
            "name": "HTML tags",
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#ff79c6"
            }
        },
        {
            "name": "JavaScript string interpolation ${}",
            "scope": [
                "punctuation.definition.template-expression.begin.ts",
                "punctuation.definition.template-expression.end.ts"
            ],
            "settings": {
                "foreground": "#ff79c6"
            }
        },
        {
            "name": "Arrow function arrows",
            "scope": "storage.type.function.arrow",
            "settings": {
                "foreground": "#ff79c6"
            }
        },
        {
            "name": "JSON keys",
            "scope": "meta.structure.dictionary.json string.quoted.double.json",
            "settings": {
                "foreground": "#8be9fd"
            }
        },
        {
            "name": "JSON string",
            "scope": "meta.structure.dictionary.json meta.structure.dictionary.value.json string.quoted.double.json",
            "settings": {
                "foreground": "#f1fa8c"
            }
        },
        {
            "name": "JSON boolean",
            "scope": "constant.language.json",
            "settings": {
                "foreground": "#50fa7b"
            }
        },
        {
            "scope": [
                "punctuation.section.embedded.begin.metatag.php",
                "punctuation.section.embedded.end.metatag.php"
            ],
            "settings": {
                "foreground": "#f8f8f2"
            }
        },
        {
            "scope": "meta.function-call.php",
            "settings": {
                "foreground": "#50fa7b"
            }
        },
        {
            "scope": "meta.structure.dictionary.key.python",
            "settings": {
                "foreground": "#8be9fd"
            }
        },
        {
            "name": "Ruby's @variable",
            "scope": "variable.other.readwrite.instance",
            "settings": {
                "foreground": "#ffb86c"
            }
        },
        {
            "scope": "string.quoted.double.shell variable",
            "settings": {
                "foreground": "#bd93f9"
            }
        },
        {
            "scope": [
                "meta.type.annotation meta.type.parameters entity.name.type",
                "meta.type.parameters entity.name.type",
                "meta.indexer.mappedtype.declaration entity.name.type",
                "meta.return.type meta.type.parameters entity.name.type"
            ],
            "settings": {
                "foreground": "#ffb86c"
            }
        },
        {
            "scope": [
                "meta.type.annotation entity.name.type",
                "meta.return.type entity.name.type"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#8be9fd"
            }
        }
    ]
}